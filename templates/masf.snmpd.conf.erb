<% if read_restrict.is_a? Array -%>
<% read_restrict.each_with_index do |source, index| -%>
<%= "com2sec masfUser#{index} #{source} #{read_community}" %>
masf0Group0 v1 masfUser<%= index %>
masf0Group0 v2 masfUser<%= index %>
<% end -%>
<% elsif read_restrict != '' -%>
<%= "com2sec masfUser0 #{read_restrict} #{read_community}" -%>
masf0Group0 v1 masfUser0
masf0Group0 v2 masfUser0
<% else -%>
com2sec masfUser0 default <%= read_community %>
<% end -%>
access masf0Group0 "" v1 noauth exact masf0View0 none none
access masf0Group0 "" v2c noauth exact masf0View0 none none
view masf0View0 included .1.3.6.1.2.1.47
view masf0View0 included .1.3.6.1.4.1.42.2.70.101

